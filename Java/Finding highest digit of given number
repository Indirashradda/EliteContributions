import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner s=new Scanner(System.in);
	    int highest=0;
	    System.out.println("Enter a number");
	    int n=s.nextInt();
		while(n>0)
		{
		    int rem=n%10;
		     n=n/10;
		     
		     if(rem>highest)
		     {
		         highest=rem;
		     }
		}
		System.out.println("The largest digit of a given number "+highest);
	}
}
